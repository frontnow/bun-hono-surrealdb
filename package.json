{
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^0.19.2",
    "hono": "^4.7.2",
    "surrealdb": "^1.2.1",
    "tslib": "^2.6.3",
    "ws": "^8.14.2",
    "zod": "^3.24.2"
  },
  "description": "A modern web API built with Hono and Bun",
