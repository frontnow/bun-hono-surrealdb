{
  "buildCommand": "bun run build",
  "env": {
    "NODEJS_HELPERS": "0"
  },
  "framework": null,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs22.x"
    }
  },
  "outputDirectory": "dist",
  "rewrites": [
    {
      "destination": "/api/index.js",
      "source": "/(.*)"
    }
  ],
  "version": 2
}
