{
  "buildCommand": "bun run build",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node_modules/**"]
      }
    }
  ],
  "installCommand": "bun install",
  "routes": [
    {
      "dest": "src/index.ts",
      "src": "/(.*)"
    }
  ],
  "version": 2
}
