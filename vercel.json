{
  "buildCommand": "bun run build",
  "env": {
    "NODE_OPTIONS": "--experimental-modules --es-module-specifier-resolution=node",
    "NODEJS_HELPERS": "0"
  },
  "framework": null,
  "functions": {
    "api/**/*": {
      "runtime": "@vercel/node"
    }
  },
  "outputDirectory": "dist",
  "rewrites": [
    {
      "destination": "/api/index.js",
      "source": "/(.*)"
    }
  ],
  "version": 2
}
